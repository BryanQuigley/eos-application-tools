[Desktop Entry]
Name=Google Chrome Initial Setup
Exec=@datadir@/@appinstallhelper@/eos-install-app-helper-installer.py --initial-setup --app-id com.google.Chrome --app-name "Google Chrome" --remote eos-apps --old-desktop-file-name google-chrome.desktop --required-archs x86_64
Type=Application
NoDisplay=true
X-GNOME-Autostart-enabled=true
AutostartCondition=unless-exists @localstatedir@/lib/@appinstallhelper@/initial-setup-done
