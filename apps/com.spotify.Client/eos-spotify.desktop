[Desktop Entry]
Version=1.0
Name=Spotify Client
Comment=Listen to music using Spotify
Exec=/usr/bin/eos-install-app-helper --app-id com.spotify.Client --remote eos-apps --app-name "Spotify Client" --old-desktop-file-name eos-spotify.desktop --new-desktop-file-name com.spotify.Client.desktop --required-archs x86_64 %U
Terminal=false
Icon=eos-spotify
Type=Application
